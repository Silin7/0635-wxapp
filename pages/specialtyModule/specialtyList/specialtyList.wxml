<van-toast id="van-toast" />

<van-tabs active="{{cityActive}}" bind:change="cityChange" ellipsis="{{false}}">
  <van-tab wx:for="{{cityList}}" wx:key="index" title="{{item.type_name}}"></van-tab>
</van-tabs>

<scroll-view style="height: {{windowHeight-44}}px;" scroll-y="true" bindscrolltolower="onScrollBottom">
  <view wx:for="{{specialtyList}}" wx:key="index">{{item.specialty_name}}</view>
</scroll-view>
