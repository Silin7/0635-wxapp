<van-toast id="van-toast" />

<!-- swiper滚动图 -->
<swiper indicator-dots="{{true}}" autoplay="{{true}}" circular="{{true}}" style="height: 360rpx;">
	<block wx:for="{{swiperList}}" wx:key="index" wx:for-item="item">
		<swiper-item bindtap="swiperTap" data-item="{{item}}">
			<image style="height: 360rpx; width: 100%;" src="{{item.image}}"></image>
		</swiper-item>
	</block>
</swiper>
<!-- 子模块01 -->
<view class="icon_module bs_bb m_t_10">
  <view class="icon_module_item" wx:for="{{iconList}}" wx:key="index" bindtap="oneNavBind" data-item="{{item}}">
    <view class="item_icon">
      <image src="{{item.image}}" class="w_h_100"></image>
    </view>
    <view class="item_text fs_18 fw_7 ls_5 p_l_5 bs_bb">{{item.name}}</view>
  </view>
</view>
