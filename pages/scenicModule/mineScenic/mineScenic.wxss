
.n_title_box_fd {
  box-sizing: border-box;
  height: 30px;
  padding: 0 10px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.footprint {
  box-sizing: border-box;
  width: 100%;
  padding: 10px;
  overflow-x: auto;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: center;
}
.footprint_item {
  box-sizing: border-box;
  flex-shrink: 0;
  width: 32%;
  height: 120px;
  margin-bottom: 10px;
  overflow: hidden;
  display: flex;
  flex-flow: column;
  justify-content: space-around;
  align-items: center;
}
.footprint_item_img {
  width: 90px;
  height: 90px;
  border-radius: 10px;
  position: relative;
}
.footprint_item_mask {
  position: absolute;
  top: 0;
  left: 0;
  width: 90px;
  height: 90px;
  border-radius: 10px;
  background-color: #999999;
  opacity: 0.7;
}
.footprint_item_text {
  box-sizing: border-box;
  width: 100%;
  height: 20px;
  color: #4545FF;
  font-size: 12px;
  font-weight: 700;
  text-align: center;
  letter-spacing: 1px;
  line-height: 20px;
}
.footprint_item_text_n {
  box-sizing: border-box;
  width: 100%;
  height: 20px;
  color: #999999;
  font-size: 12px;
  font-weight: 700;
  text-align: center;
  letter-spacing: 1px;
  line-height: 20px;
}